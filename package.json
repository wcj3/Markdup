{
  "name": "markdup",
  "version": "0.1.2",
  "description": "A programmatic way to display HTML markdup",
  "main": "lib/markdup.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wcj3/Markdup.git"
  },
  "author": "Will Johnson",
  "license": "MIT",
  "scripts": {
    "start": "gulp browsersync | npm run webpack",
    "serve:github": "gulp browersync-github",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.babel.js --watch",
    "build": "./node_modules/.bin/webpack --config webpack.prod.config.babel.js --optimize-minimize -p && gulp mover",
    "format": "js-beautify --editorconfig  --f *.js src/*.js test/*.js --replace",
    "test": "karma start karma.conf.babel.js"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "browser-sync": "^2.18.8",
    "css-loader": "^0.26.2",
    "cssnano": "^3.10.0",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-size": "^2.1.0",
    "gulp-strip-comments": "^2.4.5",
    "highlight.js": "^9.9.0",
    "jasmine-core": "^2.5.2",
    "js-beautify": "^1.6.11",
    "json-loader": "^0.5.4",
    "karma": "^1.4.1",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.3",
    "postcss-nesting": "^2.3.1",
    "style-loader": "^0.13.1",
    "sugarss": "^0.2.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "uglifyjs-webpack-plugin": "^0.3.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "benchmark": "^2.1.3",
    "highlight.js": "^9.9.0",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0"
  }
}
